{
  "name": "Strings-Sessions-Generator",
  "description": "A Telegram Bot to generate Telethon and Pyrogram String Sessions.",
  "logo": "https://te.legra.ph/file/2a001da0da57f3ccef26b.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
  "repository": "https://github.com/Spydyrkt/StringSession",
  "env": {
    "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": "7449299"
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": "e0d2656f669092faaf59579dd2b00314"
    },
    "OWNER_ID": {
      "description": "Telegram id of the person who will known as owner of the bot.",
      "required": true,
      "value": "5324352045"
    },
    "MUST_JOIN": {
      "description": "Username of a group or channel for which you want to set force sub.(without @)",
      "required": true,
      "value": "GJ516_DISCUSS_GROUP"
    },
    "BOT_TOKEN": {
      "description": "Get this value from @BotFather",
      "required": true,
      "value": ""
    }
  }
}
